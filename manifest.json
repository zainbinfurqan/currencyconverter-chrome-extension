{
  "version": "1.2.4",
  "manifest_version": 3,
  "name": "Currency Converter Extension (Crypto/Normal)",
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon-16.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon-16.png"
    },
    "default_popup": "js/index.html",
    "default_title": "Currency Converter Extension"
  },
  "permissions": ["storage"]
}
